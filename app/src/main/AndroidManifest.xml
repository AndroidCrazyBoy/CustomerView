<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.baolx.customerview" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".customer_switch.CustomerSwitchActivity" />
        <activity android:name=".slide_item.SlideItemActivity" />
        <activity android:name=".processview.ProcessActivity" />
        <activity android:name=".bottombar.BottomBarActivity" />
    </application>

</manifest>
